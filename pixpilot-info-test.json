{
  "$schema": "https://unpkg.com/@pixpilot/scaffoldfy/schema",
  "description": "Test pixpilot info - modified condition for testing",
  "name": "pixpilot-info-test",
  "dependencies": ["project-info"],
  "variables": [
    {
      "id": "pixpilot_project",
      "value": {
        "type": "conditional",
        "condition": "repoOwner === 'pixpilot'",
        "ifTrue": true,
        "ifFalse": false
      }
    }
  ]
}
